{
  "name": "kysd-cms-backend",
  "version": "1.0.0",
  "description": "KYSD CMS Backend API",
  "main": "server.js",
  "prisma": {
    "schema": "./prisma/schema.prisma"
  },
  "scripts": {
    "start": "npx prisma db push && node prisma/seed.js && node server.js",
    "dev": "nodemon server.js",
    "build": "prisma generate && prisma db push",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "node prisma/seed.js",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-validator": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.12",
    "prisma": "^5.22.0",
    "slugify": "^1.6.6",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "nodemon": "^3.1.7"
  }
}
